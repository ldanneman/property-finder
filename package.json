{
  "name": "property-finder",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "codegen": "apollo client:codegen --target typescript --localSchemaFile schema.gql --outputFlat --includes \"{pages,src}/**\" --excludes \"src/generated/**\" src/generated",
    "codegen:watch": "yarn codegen --watch",
    "db:init": "yarn prisma init",
    "db:generate": "yarn prisma generate",
    "db:migrate:new": "yarn prisma migrate dev",
    "db:migrate:up": "yarn prisma migrate up --experimental",
    "db:migrate:down": "yarn prisma migrate down --experimental",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/client": "^3.2.5",
    "@prisma/client": "^3.11.1",
    "@reach/combobox": "^0.16.5",
    "apollo-server-micro": "^2.19.0",
    "class-validator": "^0.13.2",
    "cloudinary": "^1.28.1",
    "cloudinary-react": "^1.7.1",
    "deepmerge": "^4.2.2",
    "firebase": "^9.6.6",
    "firebase-admin": "^10.0.2",
    "geolib": "^3.3.3",
    "graphql": "^15.3.0",
    "js-cookie": "^3.0.1",
    "mapbox-gl": "^2.7.1",
    "micro": "^9.3.4",
    "next": "12.0.10",
    "postcss": "^8.4.6",
    "postcss-import": "^14.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-firebaseui": "^6.0.0",
    "react-hook-form": "^7.27.0",
    "react-map-gl": "^7.0.6",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "use-debounce": "^7.0.1",
    "use-google-maps-script": "^0.1.2",
    "use-places-autocomplete": "^1.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.2",
    "@types/js-cookie": "^3.0.1",
    "@types/node": "17.0.17",
    "@types/react": "17.0.39",
    "@types/react-map-gl": "^6.1.1",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "apollo": "^2.31.1",
    "autoprefixer": "^10.4.2",
    "babel-plugin-parameter-decorator": "^1.0.16",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^8.9.0",
    "eslint-config-next": "12.0.10",
    "eslint-config-universe": "^10.0.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.3.4",
    "postcss-cli": "^9.1.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^7.3.3",
    "prettier": "^2.5.1",
    "prisma": "^3.11.1",
    "tailwindcss": "^3.0.22",
    "typescript": "4.5.5"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "yarn run prettier --write"
    ]
  }
}
